{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/Front/CSS/(.*)", "dest": "/Front/CSS/$1" },
    { "src": "/Front/JS/(.*)", "dest": "/Front/JS/$1" },
    { "src": "/Front/Assets/(.*)", "dest": "/Front/Assets/$1" },
    { "src": "/Front/Content/(.*)", "dest": "/Front/Content/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
