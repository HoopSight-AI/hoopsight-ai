{
  "version": 2,
  "builds": [
    {
      "src": "Front/Front/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    { "source": "/api/(.*)", "destination": "/Front/api/index.php" },
    { "source": "/Front/Assets/:path*", "destination": "/Front/Front/Assets/:path*" },
    { "source": "/Front/CSS/:path*", "destination": "/Front/Front/CSS/:path*" },
    { "source": "/Front/JS/:path*", "destination": "/Front/Front/JS/:path*" },
    { "source": "/", "destination": "/Front/Front/index.php" },
    { "source": "/(.*)", "destination": "/Front/Front/index.php" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
