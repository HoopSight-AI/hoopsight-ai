{
  "version": 2,
  "builds": [
    {
      "src": "Front/Front/index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/Front/Front/api/index.php" },
    { "src": "/Assets/:path*", "dest": "/Front/Front/Assets/:path*" },
    { "src": "/CSS/:path*", "dest": "/Front/Front/CSS/:path*" },
    { "src": "/JS/:path*", "dest": "/Front/Front/JS/:path*" },
    { "src": "/favicon.ico", "dest": "/Front/Front/Assets/ball-icon.ico" },
    { "src": "/", "dest": "/Front/Front/index.php" },
    { "src": "/(.*)", "dest": "/Front/Front/index.php" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
