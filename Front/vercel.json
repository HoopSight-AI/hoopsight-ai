{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/index.php" },
    { "source": "/Front/Assets/:path*", "destination": "/Front/Assets/:path*" },
    { "source": "/Front/CSS/:path*", "destination": "/Front/CSS/:path*" },
    { "source": "/Front/JS/:path*", "destination": "/Front/JS/:path*" },
    { "source": "/Front/Content/:path*", "destination": "/Front/Content/:path*" },
    { "source": "/(.*)", "destination": "/api/index.php" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
