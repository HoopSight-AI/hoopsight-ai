{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.php" },
    { "src": "/Assets/(.*)", "dest": "/Front/Assets/$1" },
    { "src": "/CSS/(.*)", "dest": "/Front/CSS/$1" },
    { "src": "/JS/(.*)", "dest": "/Front/JS/$1" },
    { "src": "/favicon.ico", "dest": "/Front/Assets/ball-icon.ico" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}
